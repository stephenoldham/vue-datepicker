{
  "name": "vue-datepicker",
  "version": "1.0.0",
  "description": "A little datepicker vue component",
  "main": "dist/vue-datepicker.ssr.js",
  "module": "dist/vue-datepicker.esm.js",
  "unpkg": "dist/vue-datepicker.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "scripts": {
    "docs": "vue-cli-service serve dev/docs.js",
    "serve": "vue-cli-service serve dev/serve.js",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js && npm run prod",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "dev": "npm run development",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "npm run development -- --watch",
    "prod": "npm run production",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stephenoldham/vue-datepicker.git"
  },
  "author": "Steve Oldham",
  "license": "MIT",
  "dependencies": {
    "@popperjs/core": "^2.4.0",
    "dayjs": "^1.8.28",
    "deepmerge": "^4.2.2",
    "rrule": "^2.6.4"
  },
  "devDependencies": {
    "@tailwindcss/ui": "^0.3.0",
    "@vue/cli-plugin-babel": "^4.3.1",
    "@vue/cli-service": "^4.3.1",
    "@vue/compiler-sfc": "^3.0.0-beta.14",
    "cross-env": "^7.0.2",
    "laravel-mix": "^5.0.4",
    "minimist": "^1.2.5",
    "prismjs": "^1.20.0",
    "rollup": "^2.15.0",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-css-only": "^2.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-vue": "^6.0.0-beta.6",
    "tailwindcss": "^1.4.6",
    "vue": "^2.6.11",
    "vue-template-compiler": "^2.6.11"
  },
  "peerDependencies": {
    "vue": "^2.6.11"
  },
  "engines": {
    "node": ">=10"
  }
}
